        "@fullhuman/postcss-purgecss": "^3.0.0",
        "@inertiajs/inertia": "^0.6.1",
        "@inertiajs/inertia-react": "^0.4.1",
        "browser-sync": "^2.26.13",
        "browser-sync-webpack-plugin": "2.0.1",
        "postcss-import": "^13.0.0",
        "postcss-nesting": "^7.0.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "sass": "^1.29.0",
